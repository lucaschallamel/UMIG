{
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "proseWrap": "always",
        "printWidth": 80,
        "tabWidth": 2,
        "useTabs": false,
        "singleQuote": false,
        "trailingComma": "none",
        "bracketSpacing": true,
        "bracketSameLine": false,
        "embeddedLanguageFormatting": "auto"
      }
    }
  ]
}